        "response = search.json()\n",
        "\n",
        "print(json.dumps(response, indent=2))\n",
        "\n",
        "with open('transcript.txt', 'w') as f:\n",
        "    for result in response.get(\"transcript\", []):\n",
        "        snippet = result.get(\"snippet\")\n",
        "        if snippet:\n",
        "            f.write(f\"{snippet}\\n\")\n",
        "\n",
        "print(\"Results saved to transcript.txt\")\n",
